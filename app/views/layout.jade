include ./logo.jade

doctype html
html(lang='en')
  head
    meta(content='text/html; charset=utf-8', http-equiv='Content-Type')
    meta(content='Fueling innovation and interdisciplinary collaboration in Tallahassee', name='description')
    meta(content='HTML, CSS, JavaScript, PHP, Minimal Awesomness', name='keywords')
    meta(content='width=device-width, initial-scale=1, maximum-scale=1', name='viewport')

    title HackFSU | #{title}

    link(href='images/favicon.png', rel='shortcut icon')
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Montserrat:400,700', rel='stylesheet', type='text/css')
    link(href='css/clear.css', rel='stylesheet', type='text/css')
    link(href='css/carouFredSel.css', rel='stylesheet', type='text/css')
    link(href='css/supersized.css', rel='stylesheet', type='text/css')
    link(href='css/common.css', rel='stylesheet', type='text/css')
    link(href='css/columns.css', rel='stylesheet', type='text/css')
    link(href='css/prettyPhoto.css', rel='stylesheet', type='text/css')
    link(href='css/accordion-tabs.css', rel='stylesheet', type='text/css')
    link(href='css/style.css', rel='stylesheet', type='text/css')
    link(href='css/isotope.css', rel='stylesheet', type='text/css')
    link(href='css/colors.css', rel='stylesheet', type='text/css')
    link(href='css/responsive.css', rel='stylesheet', type='text/css')

    //if lt IE 9
      script(src='http://html5shiv.googlecode.com/svn/trunk/html5.js')
  body
    // Loading gif
    table.doc-loader
      tr
        td
          img(alt='Loading...', src='images/ajax-document-loader.gif')
    // Navigation Menu
    #main-menu.main-menu
      .center-relative.menu-holder
        .menu-logo.left
          img(alt='Sensa', src='images/menu_logo.png')
        ul.right.montserrat-font
          li.active
            a(href='#dry-home') HELLO
          li
            a(href='#about') ABOUT US
          li
            a(href='#sponsors') SPONSORS
          li
            a(href='#contact') CONTACT US
        .clear
    block content
    block map
    // Footer
    .clear.center-text.footer
      .scroll-top-holder.relative.block.content-960.center-relative
      .footer-content.center-text.block.content-960.center-relative
        img(alt='', src='images/footer_logo.png')
        .title-separator
        .big-social.center-text
          a(href='https://twitter.com/HackFSU', style='text-decoration:none;')
            img(src='images/twitter-logo.png', alt='HackFSU on Twitter')
          a(href='https://www.facebook.com/hackfsu', style='text-decoration:none;')
            img(src='images/facebook-logo.png', alt='HackFSU on Facebook')
          a(href='http://statigr.am/tag/hackfsu', style='text-decoration:none;')
            img(src='images/instagram-logo.png', alt='HackFSU on Instagram')
        .footer-copyright.center-text
          | Created by: 
          a(href='http://www.technolegy.org/') TechNOLE
          |  at Florida State University
  script(type='text/javascript', src='http://www.parsecdn.com/js/parse-1.2.17.min.js')
  script(src='js/jquery-1.8.1.min.js', type='text/javascript')
  script(src='js/jquery.isotope.min.js', type='text/javascript')
  script(src='js/accordion-tabs.min.js', type='text/javascript')
  script(src='js/portfolio.js', type='text/javascript')
  script(src='js/supersized.3.2.7.js', type='text/javascript')
  script(src='js/jquery.sticky.js', type='text/javascript')
  script(src='js/jquery.dryMenu.js', type='text/javascript')
  script(src='js/small-menu.js', type='text/javascript')
  script(src='js/jquery.carouFredSel-6.2.1-packed.js', type='text/javascript')
  script(src='js/jquery.mousewheel.min.js', type='text/javascript')
  script(src='js/jquery.touchSwipe.min.js', type='text/javascript')
  script(src='js/jquery.easing.1.3.js', type='text/javascript')
  script(src='js/jquery.myHint.js', type='text/javascript')
  script(src='js/jquery.vticker-min.js', type='text/javascript')
  script(src='js/jquery.prettyPhoto.js', type='text/javascript')
  script(src='js/main.js', type='text/javascript')
  script(src='js/maps.js', type='text/javascript')
  script(async defer src='https://maps.googleapis.com/maps/api/js?key=AIzaSyAPFA-G6fLqiXxbhnZayxz7ahMZWoMM9X8&callback=initMap')
